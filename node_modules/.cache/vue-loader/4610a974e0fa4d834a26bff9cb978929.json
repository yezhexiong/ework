  >\n          <van-col span=\"24\">\n            <van-search\n              v-model=\"searchKey\"\n              show-action\n              placeholder=\"请输入搜索关键词\"\n              @search=\"onSearch\"\n              @cancel=\"onHidenSearch\"\n            />\n          </van-col>\n        </van-row>\n      </span>\n    </div>\n    <van-list\n      style=\"margin: 50px 0 50px 0\"\n      v-model=\"loading\"\n      :finished=\"finished\"\n      finished-text=\"没有更多了\"\n      @load=\"onLoad\"\n    >\n      <van-cell v-for=\"item in list\" :key=\"item\" :title=\"item\" />\n    </van-list>\n  </div>\n</template>\n<script>\nimport { Cell } from \"vant\";\nimport { List } from \"vant\";\nimport { Search } from \"vant\";\nimport { Icon } from \"vant\";\nimport { Col, Row } from \"vant\";\n// import { Toast } from \"vant\";\nexport default {\n  components: {\n    [List.name]: List,\n    [Cell.name]: Cell,\n    // [NavBar.name]: NavBar,\n    [Search.name]: Search,\n    [Icon.name]: Icon,\n    [Col.name]: Col,\n    [Row.name]: Row,\n  },\n  data() {\n    return {\n      active: 0,\n      list: [],\n      loading: false,\n      finished: false,\n      searchKey: \"yezhexiong\",\n      searchVisible: false,\n    };\n  },\n  methods: {\n    onLoad() {\n      // 异步更新数据\n      // setTimeout 仅做示例，真实场景中一般为 ajax 请求\n      setTimeout(() => {\n        for (let i = 0; i < 5; i++) {\n          this.list.push(this.list.length + 1);\n        }\n\n        // 加载状态结束\n        this.loading = false;\n\n        // 数据全部加载完成\n        if (this.list.length >= 40) {\n          this.finished = true;\n        }\n      }, 1000);\n    },\n    onSearch(){\n      this.searchVisible = false\n    },\n    onHidenSearch(){\n      this.searchVisible = false\n    },\n    onShowSearch(){\n      this.searchVisible = true\n    },\n    // onClickLeft() {\n    //   Toast(\"返回\");\n    // },\n    // onClickRight() {\n    //   Toast(\"按钮\");\n    // },\n  },\n};\n</script>\n\n<style lang=\"less\">\n// .ework-workbench-index {\n//   .ework-head-fixed {\n//     position: fixed;\n//     width: 100%;\n//     top: 0;\n//     height: 54px;\n//     z-index: 1;\n//     background: #eee;\n//     border-bottom: solid 1px #eee;\n//   }\n//   .van-tabbar {\n//     border-top: solid 1px #eee;\n//   }\n// }\n</style>\n"]}]}